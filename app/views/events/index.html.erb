<div data-ng-controller="MapCtrl">
  
  <div class="map">
    <div id="map-canvas"></div>
  </div>

  <!-- <p ng-init="events=<%#= @events.to_json %>"></p>
  {{events}} -->
  <h1>Events#index</h1>
  <p>Find me in app/views/events/index.html.erb</p>
     
  <div id="map-canvas"></div>
  <ul>
  <% @events.each do |event| %>

      <li>
          <%= event.title %> | <%= event.date %><br>
          Postal Code: <%= event.location %><br>
          Venue: <%= event.venue %><br>
          Lat: <%= event.lat %>, Lng: <%= event.long %><br>

          <ul>
          <% event.event_artists.each do |e| %>
              <li><%= e.artist.name %></li>
          <% end %>
          </ul>

      </li>
      <br>

  <% end %>
  </ul>


</div>
    